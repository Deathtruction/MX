{% extends 'base.html.twig' %}

{% block inscription %}

    <section class="container main-content">
        <div class="row">
            <div class="col-md-9">
                <div class="page-content">
                    <div class="boxedtitle page-title"><h2>Fiche d'inscription</h2></div>

                    <div class="form-style form-style-4">
                        <form action="{{ path('inscription') }}" method="post">
                            <div class="form-inputs clearfix">
                                <p>
                                    <label>Nom :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Prénom :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Date de Naissance :<span>*</span></label>
                                    <input type="date">
                                </p>
                                <p>
                                    <label>Adresse :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Cp :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Ville :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Téléphone :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Pseudo :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label class="required">Mail :<span>*</span></label>
                                    <input type="email">
                                </p>
                                <p>
                                    <label class="required">Mot de passe :<span>*</span></label>
                                    <input type="password" value="">
                                </p>
                                <p>
                                    <label class="required">Confirmer mot de passe :<span>*</span></label>
                                    <input type="password" value="">
                                </p>
                                <p>
                                    <label>Club :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Licence UFOLEP :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>CASM :<span>*</span></label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Permis Moto :</label>
                                    <input type="text">
                                </p>
                                <p>
                                    <label>Numéro de dossard :</label>
                                    <input type="text">
                                </p>
                            </div>
                            <p class="form-submit">
                                <input type="submit" value="Valider" class="button color small login-submit submit">
                            </p>
                        </form>
                    </div>
                </div><!-- End page-content -->
            </div><!-- End main -->


            <aside class="col-md-3 sidebar">


            </aside><!-- End sidebar -->
        </div><!-- End row -->
    </section><!-- End container -->

{% endblock %}